  function setearProporciones() {
        try {
        const tables = document.querySelectorAll('.shop_table.woocommerce-checkout-review-order-table');
        tables.forEach(table => {
            const columnas = table.querySelectorAll('thead th');

            columnas.forEach((columna, index) => {
                let proporcion;
                if (index === 0) {
                    proporcion = '80%'; // Primera columna
                } else {
                    proporcion = '20%'; // Segunda columna
                }
                columna.style.width = proporcion;
