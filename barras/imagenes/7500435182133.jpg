sliderContainer.style.alignItems = 'center'; // Alinea los elementos verticalmente en el centro

//sliderContainer.appendChild(nextArrow);

prevArrow.addEventListener('click', () => {
    const width = sliderContainer.offsetWidth;
    if (currentTranslateX < 0) {
        currentTranslateX += 30;
        slider.style.transition = 'transform 0.3s ease'; // Transición suave
        slider.style.transform = `translateX(${currentTranslateX}px)`;
    }
});

nextArrow.addEventListener('click', () => {
    const width = sliderContainer.offsetWidth;
    const sliderWidth = slider.offsetWidth;
    if (Math.abs(currentTranslateX) < sliderWidth - width) {
        currentTranslateX -= 30;
        slider.style.transition = 'transform 0.3s ease'; // Transición suave
        slider.style.transform = `translateX(${currentTranslateX}px)`;
    }
}

);


let slider = document.querySelector('.wpclv-terms');

let isMouseDown = false;
let startX;
let translateX = 0;
let currentTranslateX = 0;

function updateSliderPosition() {
    slider.style.transform = `translateX(${translateX}px)`;
}

function startDragging(e) {
    isMouseDown = true;
    startX = e.clientX - slider.offsetLeft;
    currentTranslateX = translateX;
}

function drag(e) {
    if (!isMouseDown) return;
    e.preventDefault();
    
    const x = e.clientX - slider.offsetLeft;
    const moveX = x - startX;
    
    
    if(currentTranslateX + moveX<=0 && currentTranslateX + moveX>=-sliderContainer.offsetWidth ){
    translateX = currentTranslateX + moveX;
    console.log(translateX)
    updateSliderPosition();
}
        
    }

    function stopDragging() {
        isMouseDown = false;
    }


    slider.addEventListener('mousedown', startDragging);
slider.addEventListener('mousemove', drag);
slider.addEventListener('mouseup', stopDragging);
slider.addEventListener('mouseleave', stopDragging);
try {
