    var firstTermsElement = $('.wpclv-terms').first();
    var colorElements = firstTermsElement.find('.wpclv-term-color');
    var colorArray = null; 
    colorArray = Array.from(colorElements);

    function compareColors(a, b) {
    const colorA = getColorCode(a);
    const colorB = getColorCode(b);
        const distanceA = calculateColorDistance(colorA);
    const distanceB = calculateColorDistance(colorB);
        return  distanceB-distanceA;
    }

    function getColorCode(element) {
    const elementhijo = element.firstChild;
    const backgroundColor = elementhijo.style.backgroundColor;
    return backgroundColor;
    }

        function calculateColorDistance(color) {
        const rgbValues = color.match(/\d+/g).map(Number);
        const referenceColor = [0, 0, 0]; // Puedes cambiar estos valores según tu necesidad
        
        // Calcula la distancia euclidiana entre los colores
        const distance = Math.sqrt(
            Math.pow(rgbValues[0] - referenceColor[0], 2) +
            Math.pow(rgbValues[1] - referenceColor[1], 2) +
            Math.pow(rgbValues[2] - referenceColor[2], 2)
        );
        
        return distance;
        }

        colorArray.sort(compareColors);
        const container = document.querySelector('.wpclv-terms');
        container.innerHTML = '';
        colorArray.forEach(function(element) {
        container.appendChild(element);
