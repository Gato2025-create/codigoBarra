<script>
document.addEventListener("DOMContentLoaded", function() {
    const wrapper = document.querySelector('.ticker-wrapper');
    const ticker = document.querySelector('.ticker-wrapper .elementor-heading-title');

    if (!wrapper || !ticker) {
        return;
    }

    const setupTicker = () => {
        const originalContent = ticker.innerHTML;
        const wrapperWidth = wrapper.offsetWidth;
        
        // Reset content to calculate original width correctly
        ticker.innerHTML = originalContent;
        let contentWidth = ticker.offsetWidth;

        // Duplicate content until it's at least double the wrapper width
        if (contentWidth > 0) {
            while (ticker.offsetWidth < wrapperWidth * 2) {
                ticker.innerHTML += originalContent;
            }
        }
        
        contentWidth = ticker.offsetWidth; // Final width after duplication

        // Use the original, working speed calculation logic
        let speedFactor;
        if (window.innerWidth > 1024) {
            speedFactor = 12; // Desktop
        } else if (window.innerWidth > 768) {
            speedFactor = 7;  // Tablet
        } else {
            speedFactor = 3;  // Mobile
        }

        const duration = (contentWidth / wrapperWidth) * speedFactor;
        ticker.style.animationDuration = duration + 's';
    };

    setupTicker();
    window.addEventListener('resize', setupTicker);
});
</script>				</div>
